{
  "name": "deepseek-client",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron . ",
    "package": "electron-builder --win"
  },
  "devDependencies": {
    "ajv": "latest",
    "ajv-keywords": "latest",
    "app-builder-bin": "^4.2.0",
    "electron": "^28.0.0",
    "electron-builder": "^26.0.12",
    "typescript": "^5.9.2"
  },
  "build": {
    "appId": "com.dolphinseanetwork.deepseekclient",
    "productName": "DeepSeek",
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}